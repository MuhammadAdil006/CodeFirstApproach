@model IEnumerable<Employee> ;

@{
    ViewData["Title"] = "Home Page";
  
}

<div class="text-center">
 

</div>
     <table>
         <thead>
             <tr>
                 <th>Id</th>
                 <th>Name</th>
             </tr>
         </thead>
         <tbody>
         @foreach(Employee e in Model)
    {
        <tr>
            <td>@e.Id</td>
            <td>@e.Name</td>
        </tr>
    }
    </tbody>
</table>           